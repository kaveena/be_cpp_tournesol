#include "type.hpp"

/*
###############################################################################
                               LA CLASSE CAPTEUR
###############################################################################
*/

capteur::capteur(unsigned int x){
  pin = x;
}

capteur::get_pin(){
  return this->pin;
}

/*
###############################################################################
                               LA CLASSE ACTIONNEUR
###############################################################################
*/
actionneur::actionneur(unsigned int x){
  this->pin = x;
}

actionneur::get_pin(){
  return this->pin;
}
